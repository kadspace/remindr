import kotlinx.datetime.LocalDateTime;

CREATE TABLE QueueNote (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    text TEXT NOT NULL,
    created_at TEXT NOT NULL
);

getAll:
SELECT * FROM QueueNote ORDER BY id DESC;

insert:
INSERT INTO QueueNote(text, created_at) VALUES (?, ?);

deleteById:
DELETE FROM QueueNote WHERE id = ?;

getLastInsertedId:
SELECT last_insert_rowid();
